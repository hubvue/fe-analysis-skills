    "analyze-dependencies": "scripts/analyze-dependencies.js"
  },
  "scripts": {
    "test": "node scripts/analyze-dependencies.js ./test-project",
    "analyze": "node scripts/analyze-dependencies.js ."
  },
  "keywords": [
    "dependencies",
    "analysis",
    "unused",
    "phantom",
    "circular",
    "security",
    "outdated",
    "bundle-optimization",
    "dependency-management"
  ],
  "author": "Claude",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "acorn": "^8.11.2",
    "acorn-walk": "^8.3.0"
  }
}